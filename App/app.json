{
  "expo": {
    "name": "call_app",
    "slug": "call_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location",
        "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to access your location",
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location"
      },
      "bundleIdentifier": "drone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "LOGIN_IOS": "595824997092-1tgu6unnskaj3vsojgiog8l2bmf7lhmk.apps.googleusercontent.com",
      "LOGIN_ANDROID": "595824997092-0hjnftbq6qcu462233il7vbtvfs7kj5c.apps.googleusercontent.com"
    },
    "plugins": [
      "expo-location"
    ]
  }
}